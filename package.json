{
  "name": "vue-app-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "vitest:unit": "vitest",
    "vitest:coverage": "vitest run --coverage",
    "vitest:ui": "vitest --ui",
    "lint": "eslint --ext .js,.ts,.vue src",
    "lint:fix": "eslint --ext .js,.ts,.vue src --fix",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,md}\"",
    "playwright:e2e": "playwright test",
    "playwright:e2e:ui": "playwright test --ui",
    "playwright:e2e:chromium": "playwright test --project=chromium",
    "playwright:e2e:file": "playwright test example",
    "playwright:e2e:debug": "playwright test --debug",
    "playwright:e2e:codegen": "playwright codegen",
    "playwright:e2e:report": "playwright show-report"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.4.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.91.0",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.20",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.6"
  },
  "dependencies": {
    "@types/node": "^24.3.0"
  }
}
